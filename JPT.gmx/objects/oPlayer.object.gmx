        <arguments>
          <argument>
            <kind>1</kind>
            <string>///check event
var onGroundPrev = onGround;

event_inherited();

// Landing after jump
if (onGround &amp;&amp; !onGroundPrev) {
    // Squash + stretch
    xscale = 1.5;
    yscale = 0.5;
    
    // Dust particles
    for (var i = 0; i &lt; 4; i++)
        instance_create(random_range(bbox_left, bbox_right), bbox_bottom, oFxDust);
}
</string>
